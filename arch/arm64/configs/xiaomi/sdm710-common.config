# Localversion
CONFIG_LOCALVERSION="-Twilight_V3"
CONFIG_LOCALVERSION_AUTO=y

# DTBO
CONFIG_BUILD_ARM64_DT_OVERLAY=y

# QCACLD
CONFIG_QCA_CLD_WLAN=y

# exFAT
CONFIG_EXFAT_FS=y

# pstore
CONFIG_CMDLINE="cgroup_disable=pressure ramoops_memreserve=4M"
CONFIG_CMDLINE_EXTEND=y
CONFIG_PSTORE=y
CONFIG_PSTORE_CONSOLE=y
CONFIG_PSTORE_PMSG=y
CONFIG_PSTORE_RAM=y
CONFIG_BLK_WBT=y
CONFIG_BLK_WBT_SQ=y
CONFIG_MTD=y
CONFIG_MTD_BLOCK=y
CONFIG_MTD_OOPS=y
CONFIG_MTD_BLOCK2MTD=y

# IR
CONFIG_MEDIA_RC_SUPPORT=y
CONFIG_LIRC=y
CONFIG_RC_DEVICES=y
CONFIG_IR_SPI=y

# initramfs
CONFIG_INITRAMFS_IGNORE_SKIP_FLAG=y

# LKMD (Low-Memory-Killer-Daemon) dependencies
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set
# CONFIG_MEMCG is not set
# CONFIG_MEMCG_SWAP is not set
# CONFIG_PSI is not set
CONFIG_ANDROID_SIMPLE_LMK=y
CONFIG_ANDROID_SIMPLE_LMK_MINFREE=128
CONFIG_ANDROID_SIMPLE_LMK_TIMEOUT_MSEC=200

# Haptics
CONFIG_LEDS_QPNP_HAPTICS=n
CONFIG_INPUT_QTI_HAPTICS=y

# vDSO
CONFIG_COMPAT_VDSO=y

# Misc
# CONFIG_BOUNCE is not set
# CONFIG_CPU_FREQ_TIMES is not set
CONFIG_FRAME_WARN=0

# Frame pointers
# CONFIG_KALLSYMS is not set
# CONFIG_FRAME_POINTER is not set

# Power efficient workqueues
CONFIG_WQ_POWER_EFFICIENT_DEFAULT=y

# Skip sync
CONFIG_SUSPEND_SKIP_SYNC=y

# VM Mapping
CONFIG_PGTABLE_MAPPING=y

# Compression
CONFIG_CRYPTO_ZSTD=y

# Zram
CONFIG_ZRAM_DEDUP=y

# KernelSU
CONFIG_KPROBES=y
CONFIG_OVERLAY_FS=y

# Pelt
# CONFIG_PELT_UTIL_HALFLIFE_32 is not set
CONFIG_PELT_UTIL_HALFLIFE_16=y
# CONFIG_SCHED_WALT is not set

# Dynamic STUNE boost
CONFIG_DYNAMIC_STUNE_BOOST=y

# TCP
CONFIG_TCP_CONG_ADVANCED=y
# CONFIG_TCP_CONG_BIC is not set
CONFIG_TCP_CONG_CUBIC=y
CONFIG_TCP_CONG_WESTWOOD=y
# CONFIG_TCP_CONG_HTCP is not set
# CONFIG_TCP_CONG_HSTCP is not set
# CONFIG_TCP_CONG_HYBLA is not set
# CONFIG_TCP_CONG_VEGAS is not set
# CONFIG_TCP_CONG_NV is not set
# CONFIG_TCP_CONG_SCALABLE is not set
# CONFIG_TCP_CONG_LP is not set
# CONFIG_TCP_CONG_VENO is not set
# CONFIG_TCP_CONG_YEAH is not set
# CONFIG_TCP_CONG_ILLINOIS is not set
# CONFIG_TCP_CONG_DCTCP is not set
# CONFIG_TCP_CONG_CDG is not set
CONFIG_TCP_CONG_BBR=y
# CONFIG_DEFAULT_CUBIC is not set
# CONFIG_DEFAULT_WESTWOOD is not set
CONFIG_DEFAULT_BBR=y
# CONFIG_DEFAULT_RENO is not set
CONFIG_DEFAULT_TCP_CONG="bbr"
